{
  "extends": ["config:recommended"],
  "additionalBranchPrefix": "renovate/",
  "baseBranches": ["main"],
  "labels": [
    "dependencies",
    "PR: Review TODO",
    "Is it no QA",
    "PR: Auto release"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["minor"],
      "automerge": true
    }
  ],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 10,
  "rangeStrategy": "bump",
  "vulnerabilityAlerts": {"labels": ["security"], "automerge": true}
}